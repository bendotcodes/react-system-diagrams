    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "storybook": "^8.3.6",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx}": "eslint --cache --fix",
